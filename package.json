{
  "name": "real-time-inventory",
  "version": "1.0.0",
  "description": "A simple inventory tracker in real-time",
  "main": "server.js",
  "scripts": {
    "scss": "node-sass --output-style compressed scss/styles.scss public/styles.css",
    "autoprefixer": "postcss -u autoprefixer -r public/styles.css",
    "cleancss": "cleancss -o public/styles.css public/styles.css",
    "image": "imagemin img/* -o public/img",
    "font": "cp font/*.* public/font/",
    "html": "cp *.html public/",
    "start": "nodemon --harmony server.js",
    "build:html": "npm run html && npm run image && npm run font",
    "build:css": "npm run scss && npm run autoprefixer && npm run cleancss",
    "build:js": "webpack",
    "build": "npm run build:html && npm run build:css && npm run build:js",
    "watch:html": "onchange \"./*.html\" -- npm run build:html",
    "watch:css": "onchange \"./scss/*.scss\" -- npm run build:css",
    "watch:js": "onchange \"./components/*.js\" -- npm run build:js",
    "watch": "parallelshell \"npm run watch:html\" \"npm run watch:css\" \"npm run watch:js\"",
    "begin": "npm run build && npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jstrother/real-time-inventory.git"
  },
  "author": "Jim Strother",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jstrother/real-time-inventory/issues"
  },
  "homepage": "https://github.com/jstrother/real-time-inventory#readme",
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.19.0",
    "express": "^4.14.0",
    "jquery": "^3.1.1",
    "material-ui": "^0.16.6",
    "normalize-scss": "^6.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "rethinkdb": "^2.3.3",
    "socket.io": "^1.7.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "clean-css": "^3.4.23",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "express": "^4.14.0",
    "imagemin": "^5.2.2",
    "jquery": "^3.1.1",
    "material-ui": "^0.16.6",
    "mocha": "^3.2.0",
    "node-sass": "^4.1.1",
    "nodemon": "^1.11.0",
    "onchange": "^3.2.1",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^2.6.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "rethinkdb": "^2.3.3",
    "sass-loader": "^4.1.1",
    "socket.io": "^1.7.2",
    "style-loader": "^0.13.1",
    "support-for": "^1.0.7",
    "uglify-js": "^2.7.5",
    "uglify-js-plugin": "0.0.6",
    "webpack": "^1.14.0"
  }
}
